<navigator class="top" url="mySetting/mySetting" redirect>
    <image class="userinfo-avator" src="{{userInfo.avatarUrl}}" background-size="cover" />
    <text class="userinfo-nickname">{{userInfo.nickName}}</text>
</navigator>

<view class="weui-cells wrui-cells_after-title content">
    <view class="flex-items">
        <navigator url="myBuy/myBuy" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__bd">我想买的</view>
            <view class="weui-cell__ft weui-cell__ft_in-access"></view>
        </navigator>
    </view>

    <view class="flex-items">
        <navigator url="mySell/mySell" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__bd">我要卖的</view>
            <view class="weui-cell__ft weui-cell__ft_in-access"></view>
        </navigator>
    </view>

    <view class="flex-items">
        <navigator url="myFollower/myFollower" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__bd">我的粉丝</view>
            <view class="weui-cell__ft weui-cell__ft_in-access"></view>
        </navigator>
    </view>
    <view class="flex-items">
        <navigator url="contactUs/contactUs" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__bd">联系我们</view>
            <view class="weui-cell__ft weui-cell__ft_in-access"></view>
        </navigator>
    </view>
</view>